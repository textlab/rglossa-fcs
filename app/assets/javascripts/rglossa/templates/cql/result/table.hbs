<div class="row-fluid search-result-table-container">
    <table class="table table-striped table-bordered">
      {{#each arrangedContent}}
          <tr>
            {{#if corpusPart}}
              <td>{{{corpusPart}}}</td>
            {{/if}}
              <td>{{{sId}}}</td>
              <td class="left-context">{{{preMatch}}}</td>
              <td>{{{match}}}</td>
              <td class="right-context">{{{postMatch}}}</td>
          </tr>
      {{/each}}
    </table>
</div>
